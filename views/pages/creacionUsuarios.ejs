<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ('../partials/head'); %>
    <title>Administraci√≥n de Usuarios</title>
	<script>
		window.onpageshow = function(){
			fetch('/validar')
			.then(result => result.json())
			.then(rs => {
				var usuario = rs.usuario
				if(!usuario){
					window.location=`/`
				}else{
					document.getElementById('UserLogin').textContent = usuario;
				}
			}) 
			.catch(error => {
				console.log("Error: ", error)
			})
		}
	</script>

</head>
